AA,QAAA,EAAA;AACA,UAAA,EAAA,GAAA,IAAA,CAAA,UAAA,EAAA;AACA,UAAA,CAAA,GAAA,IAAA,CAAA,UAAA,EAAA;AACA,aACA,CAAA,GACA,GADA,GAEA,KAAA,IAAA,CAAA,CAAA,CAFA,GAGA,GAHA,GAIA,KAAA,IAAA,CAAA,CAAA,CAJA,GAKA,GALA,GAMA,KAAA,IAAA,CAAA,CAAA,CANA,GAOA,GAPA,GAQA,KAAA,IAAA,CAAA,EAAA,CARA,GASA,GATA,GAUA,KAAA,IAAA,CAAA,CAAA,CAXA;AAaA,KAvBA;AAwBA,IAAA,IAxBA,gBAwBA,CAxBA,EAwBA;AACA,aAAA,CAAA,GAAA,EAAA,GAAA,MAAA,CAAA,GAAA,CAAA;AACA,KA1BA;AA2BA,IAAA,MA3BA,kBA2BA,GA3BA,EA2BA;AACA,UAAA,KAAA,KAAA,CAAA,WAAA,CAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,MAAA,KAAA,KAAA,CAAA,WAAA,CAAA,IAAA,GAAA,QAAA,GAAA,GAAA;AACA;AACA;AA/BA;AAzBA,CAAA","sourcesContent":["<template>\n  <div class=\"my-home\">\n    <section class=\"main-content\">\n      <div class=\"blog-content\">\n        <template v-for=\"(tag,index) in blog\" v-if=\"index<infoLength\">\n          <Article :tag=\"tag\" @turnTo=\"change\"></Article>\n        </template>\n        <div class=\"more\" v-if=\"show\" @click=\"infoLength=blog.length;show=false\">查看全部<span class='reform-xiala'></span></div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport Article from \"@theme/components/Article.vue\";\nlet defaultLength = 10; //默认显示条数\nexport default {\n  components: {\n    Article\n  },\n  data() {\n    return {\n      blog: [], //博客\n      infoLength: defaultLength, //默认显示条数\n      show: false\n    };\n  },\n  mounted() {\n    let go = this.$site.pages.filter(v => {\n      if (v.title) {\n        return v;\n      }\n    });\n    go = go.sort((pre, next) => {\n      return new Date(pre.lastUpdated) > new Date(next.lastUpdated) ? -1 : 1;\n    });\n    this.blog = go;\n    if (this.blog.length > defaultLength) {\n      this.show = true;\n    }\n  },\n  methods: {\n    format(timer) {\n      //shijianchuo是整数，否则要parseInt转换\n      var time = new Date(timer);\n      var y = time.getFullYear();\n      var m = time.getMonth() + 1;\n      var d = time.getDate();\n      var h = time.getHours();\n      var mm = time.getMinutes();\n      var s = time.getSeconds();\n      return (\n        y +\n        \"-\" +\n        this.add0(m) +\n        \"-\" +\n        this.add0(d) +\n        \" \" +\n        this.add0(h) +\n        \":\" +\n        this.add0(mm) +\n        \":\" +\n        this.add0(s)\n      );\n    },\n    add0(m) {\n      return m < 10 ? \"0\" + m : m;\n    },\n    change(tag) {\n      if (this.$site.themeConfig.tags) {\n        this.$router.push(\"/\" + this.$site.themeConfig.tags + \"/?tag=\" + tag);\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n$color =$accentColor;\n\n.my-home {\n  padding-top: 5rem;\n  width:100%;\n  .main-content {\n    width: 100%;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n\n    // background-color max-width:50rem;\n    .blog-content {\n      width: 80%;\n\n      .more {\n        color: $color;\n        text-align: center;\n        padding: 1rem 0;\n        cursor: pointer;\n        // text-decoration: underline;\n      }\n    }\n  }\n}\n\n@media (max-width: 719px) {\n  .home .main-content .blog-content {\n    width: 100%;\n  }\n}\n</style>"],"sourceRoot":"node_modules/vuepress-theme-reform/components"}]}